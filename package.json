{
  "name": "web-puranatura---terapias-naturales",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "concurrently \"npx vite\" \"cd backend && npm run dev\" --names \"FRONT,BACK\" --prefix-colors \"cyan,magenta\"",
    "build": "npm run optimize-images && npx tsc && npx vite build",
    "ci:build": "npm run optimize-images && npx vite build",
    "lint": "eslint --no-error-on-unmatched-pattern src scripts test --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --no-error-on-unmatched-pattern src scripts test --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:frontend": "tsc -p tsconfig.frontend.json --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "maintenance:db": "cd backend && ts-node --transpile-only scripts/db-maintenance.ts",
    "test:ci": "cross-env-shell \"node ./scripts/cleanup-prisma-tmp.cjs && cd backend && npx prisma migrate deploy --schema=prisma/schema.prisma && cd .. && vitest run\"",
    "generate:sbom": "npx @cyclonedx/cdxgen -o sbom.json",
    "check:licenses": "node scripts/check-licenses.cjs",
    "check:openapi-drift": "node scripts/check-openapi-drift.cjs",
    "check:secret-drift": "node scripts/check-secret-drift.cjs",
    "check:forbidden-artifacts": "node scripts/check-forbidden-artifacts.cjs",
    "check:setup-plan": "node scripts/setup-plan.cjs",
    "backup:safe": "node backend/scripts/safe-backup.cjs",
    "test:breaker": "node scripts/test-breaker.cjs",
    "test:unit": "vitest run",
    "docs:regen": "node ./scripts/generate-phase-checkpoints.cjs",
    "test:e2e": "npm run build && node scripts/run-e2e.cjs",
    "test:contract": "node scripts/run-contract.cjs",
    "lint:openapi": "npx @stoplight/spectral-cli lint GPT-51-Codex-Max/api/openapi.yaml --fail-severity error",
    "deploy:check": "node scripts/deploy-check.cjs",
    "perf:api": "node scripts/run-k6.cjs GPT-51-Codex-Max/perf/k6-api-smoke.js",
    "perf:web": "npx lhci autorun --config=GPT-51-Codex-Max/perf/lhci-config.json",
    "lighthouse:local": "node ./scripts/run-lighthouse-local.cjs",
    "a11y": "node scripts/run-a11y-scan.cjs",
    "regression": "node scripts/run-regression.cjs",
    "scan:security": "node scripts/run-gitleaks.cjs && node scripts/run-trivy.cjs",
    "audit": "audit-ci --moderate",
    "ci:lint": "eslint --no-error-on-unmatched-pattern src scripts test --ext .ts,.tsx --max-warnings 0",
    "ci:typecheck": "tsc --noEmit",
    "ci:test": "vitest run --coverage",
    "ci:a11y": "node scripts/run-a11y-scan.cjs",
    "ci": "npm run ci:lint && npm run ci:typecheck && npm run ci:test",
    "check:no-secrets": "node scripts/check-no-secrets.cjs",
    "optimize-images": "node --loader ts-node/esm scripts/optimizeImages.ts",
    "rollout:canary": "node scripts/rollout-canary.cjs",
    "verify:observability": "node scripts/verify-observability-artifacts.cjs",
    "synthetic:checks": "node --loader ts-node/esm scripts/synthetic-checks.ts",
    "metrics:update": "node --loader ts-node/esm scripts/update-metrics.ts",
    "check:complexity": "node scripts/check-complexity.cjs",
    "check:bundle": "node scripts/check-bundle-size.cjs"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,cjs,mjs,json,md,css,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@hookform/resolvers": "^5.2.1",
    "@react-hook/intersection-observer": "^3.1.2",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^19.1.9",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.29",
    "axios": "^1.11.0",
    "dompurify": "^3.2.6",
    "immer": "^10.1.1",
    "lucide-react": "^0.561.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.0",
    "sharp": "^0.34.3",
    "vite-imagetools": "^7.1.0",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.41.0",
    "@redocly/cli": "^2.11.1",
    "@stoplight/prism-cli": "^5.14.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.14.0",
    "@types/react-dom": "^19.1.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "adm-zip": "^0.5.16",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^14.5.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "glob": "^13.0.0",
    "happy-dom": "^20.0.10",
    "husky": "^8.0.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.3",
    "lint-staged": "^16.2.7",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "rollup-plugin-visualizer": "^5.8.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.3"
  }
}
