{
  "name": "web-puranatura---terapias-naturales",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "npx vite",
    "build": "npm run optimize-images && npx tsc && npx vite build",
    "lint": "eslint src pages components tools scripts test --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src pages components tools scripts test --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "cross-env-shell \"DATABASE_URL=file:./prisma/dev.db\" \"node ./scripts/cleanup-prisma-tmp.cjs && cd backend && npx prisma migrate deploy --schema=prisma/schema.prisma && cd .. && vitest run\"",
    "generate:sbom": "npx @cyclonedx/cdxgen -o sbom.json",
    "check:licenses": "node scripts/check-licenses.cjs",
    "check:openapi-drift": "node scripts/check-openapi-drift.cjs",
    "check:secret-drift": "node scripts/check-secret-drift.cjs",
    "check:setup-plan": "node scripts/setup-plan.cjs",
    "backup:safe": "node backend/scripts/safe-backup.cjs",
    "test:breaker": "node scripts/test-breaker.cjs",
    "test:unit": "vitest run",
    "docs:regen": "node ./scripts/generate-phase-checkpoints.cjs",
    "test:e2e": "npm run build && node scripts/run-e2e.cjs",
    "test:contract": "node scripts/run-contract.cjs",
    "lint:openapi": "npx @stoplight/spectral-cli lint GPT-51-Codex-Max/api/openapi.yaml --fail-severity error",
    "deploy:check": "node scripts/deploy-check.cjs",
    "perf:api": "node scripts/run-k6.cjs GPT-51-Codex-Max/perf/k6-api-smoke.js",
    "perf:web": "npx lhci autorun --config=GPT-51-Codex-Max/perf/lhci-config.json",
    "lighthouse:local": "node ./scripts/run-lighthouse-local.cjs",
    "a11y": "node scripts/run-a11y.cjs",
    "scan:security": "node scripts/run-gitleaks.cjs && node scripts/run-trivy.cjs",
    "check:no-secrets": "node scripts/check-no-secrets.cjs",
    "optimize-images": "node --loader ts-node/esm scripts/optimizeImages.ts",
    "rollout:canary": "node scripts/rollout-canary.cjs",
    "verify:observability": "node scripts/verify-observability-artifacts.cjs",
    "synthetic:checks": "node --loader ts-node/esm scripts/synthetic-checks.ts",
    "check:complexity": "node scripts/check-complexity.cjs"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md,json,css,scss,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@react-hook/intersection-observer": "^3.1.2",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.1.9",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.29",
    "axios": "^1.11.0",
    "dompurify": "^3.2.6",
    "express": "^5.1.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.17.0",
    "nanoid": "^5.1.5",
    "next-seo": "^6.8.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.0",
    "sharp": "^0.34.3",
    "vite-imagetools": "^7.1.0",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^0.1.0",
    "@playwright/test": "^1.41.0",
    "@redocly/cli": "^2.11.1",
    "@stoplight/prism-cli": "^5.14.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.14.0",
    "@types/react-dom": "^19.1.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^14.5.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "glob": "^13.0.0",
    "happy-dom": "^20.0.10",
    "husky": "^8.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.2.7",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "rollup-plugin-visualizer": "^5.8.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.3"
  }
}
